/dts-v1/;

/include/ "msm8610.dtsi"

/ {
	model = "Qualcomm MSM 8610 Rumi";
	compatible = "qcom,msm8610-rumi", "qcom,msm8610", "qcom,rumi";
	qcom,board-id = <15 0>;
