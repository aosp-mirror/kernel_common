/dts-v1/;

/include/ "msm8974-v1.dtsi"
/include/ "msm8974-fluid.dtsi"

/ {
	model = "Qualcomm MSM 8974 FLUID";
	compatible = "qcom,msm8974-fluid", "qcom,msm8974", "qcom,fluid";
	qcom,msm-id = <126 3 0>,
		      <185 3 0>,
		      <186 3 0>;

};

&pm8941_chg {
	qcom,charging-disabled;
};

&sdcc1 {
	qcom,bus-width = <4>;
};
