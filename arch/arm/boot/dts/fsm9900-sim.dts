/dts-v1/;

/include/ "fsm9900.dtsi"

/ {
	model = "Qualcomm FSM9900 Simulator";
	compatible = "qcom,fsm9900-sim", "qcom,fsm9900", "qcom-sim";
	qcom,msm-id = <188 0 0>;

	aliases {
		serial0 = &uart0;
	};
};

&soc {
	uart0: serial@f9960000 {
		interrupts = <0 116 0>;
		status = "ok";
	};
};
