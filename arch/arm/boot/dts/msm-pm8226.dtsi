			regulator-name = "8226_l2";
			reg = <0x4100 0x100>;
			compatible = "qcom,qpnp-regulator";
			status = "disabled";
		};

		regulator@4200 {
			regulator-name = "8226_l3";
			reg = <0x4200 0x100>;
			compatible = "qcom,qpnp-regulator";
