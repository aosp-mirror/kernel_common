	};

	gpio@da00 { /* GPIO 27 */
		qcom,mode = <1>;		/* QPNP_PIN_MODE_DIG_OUT */
		qcom,output-type = <0>;		/* QPNP_PIN_OUT_BUF_CMOS */
		qcom,pull = <5>;		/* QPNP_PIN_PULL_NO */
		qcom,vin-sel = <2>;		/* QPNP_PIN_VIN2 */
		qcom,out-strength = <1>;	/* QPNP_PIN_OUT_STRENGTH_LOW */
		qcom,src-sel = <0>;		/* QPNP_PIN_SEL_FUNC_CONSTANT */
		qcom,master-en = <1>;
	};

	gpio@db00 { /* GPIO 28 */
		qcom,mode = <2>;
	};

	gpio@dc00 { /* GPIO 29 */
		qcom,mode = <1>;		/* QPNP_PIN_MODE_DIG_OUT */
		qcom,invert = <0>;
		qcom,out-strength = <1>;	/* QPNP_PIN_OUT_STRENGTH_LOW */
		qcom,src-sel = <0>;		/* QPNP_PIN_SEL_FUNC_1  */
		qcom,master-en = <1>;
	};

	gpio@dd00 { /* GPIO 30 */
		qcom,mode = <1>;		/* QPNP_PIN_MODE_DIG_OUT */
		qcom,invert = <0>;
		qcom,pull = <5>;		/* QPNP_PIN_PULL_NO */
		qcom,master-en = <1>;
	};

	gpio@de00 { /* GPIO 31 */
		qcom,mode = <2>;
		qcom,pull = <5>;
		qcom,vin-sel = <2>;
		qcom,src-sel = <2>;
	};

	gpio@df00 { /* GPIO 32 */
		qcom,mode = <1>;        	/* QPNP_PIN_MODE_DIG_IN */
		qcom,pull = <5>;		/* QPNP_PIN_PULL_NO */
		qcom,master-en = <1>;
	};

	gpio@e000 { /* GPIO 33 */
		qcom,mode = <1>;		/* QPNP_PIN_MODE_DIG_OUT */
		qcom,invert = <0>;
		qcom,pull = <5>;		/* QPNP_PIN_PULL_NO */
		qcom,vin-sel = <2>;
		qcom,out-strength = <1>;
		qcom,master-en = <1>;
	};

	gpio@e100 { /* GPIO 34 */
		qcom,mode = <1>;		/* QPNP_PIN_MODE_DIG_OUT */
		qcom,inver = <1>;
		qcom,pull = <5>;		/* QPNP_PIN_PULL_NO */
		qcom,out-strength = <1>;
		qcom,vin-sel = <2>;
		qcom,master-en = <1>;
	};

	gpio@e200 { /* GPIO 35 */
		qcom,mode = <1>;		/* QPNP_PIN_MODE_DIG_IN */
		qcom,pull = <5>;		/* QPNP_PIN_PULL_NO */
		qcom,master-en = <1>;
	};

	gpio@e300 { /* GPIO 36 */
		qcom,mode = <1>;
		qcom,invert = <0>;
		qcom,master-en = <1>;
	};
};

&pm8941_mpps {

	mpp@a000 { /* MPP 1 */
		status = "disabled";
	};

	mpp@a100 { /* MPP 2 */
		qcom,mode = <1>;		/* QPNP_PIN_MODE_DIG_OUT */
		qcom,invert = <0>;
		qcom,out-strength = <1>;	/* QPNP_PIN_OUT_STRENGTH_LOW */
		qcom,master-en = <0>;
	};

	mpp@a200 { /* MPP 3 */
		qcom,pull = <0>;
		qcom,src-sel = <2>;
		qcom,master-en = <1>;
	};

	mpp@a300 { /* MPP 4 */
		qcom,mode = <1>;
		qcom,master-en = <0>;
	};

	mpp@a400 { /* MPP 5 */
		qcom,mode = <1>;
		qcom,master-en = <0>;
	};

	mpp@a500 { /* MPP 6 */
		qcom,mode = <1>;		/* DIG_IN */
		qcom,invert = <0>;
		qcom,output-type = <0>;		/* CMOS */
		qcom,vin-sel = <2>;		/* PM8941_S3 1.8V > 1.6V */
		qcom,src-sel = <0>;		/* CONSTANT */
		qcom,out-strength = <1>;	/* QPNP_PIN_OUT_STRENGTH_LOW */
		qcom,master-en = <0>;		/* ENABLE MPP */
	};

	mpp@a600 { /* MPP 7 */
		qcom,mode = <1>;		/* QPNP_PIN_MODE_DIG_OUT */
		qcom,out-strength = <1>;	/* QPNP_PIN_OUT_STRENGTH_LOW */
		qcom,master-en = <0>;
	};

	mpp@a700 { /* MPP 8 */
		qcom,mode = <1>;		/* QPNP_PIN_MODE_DIG_OUT */
		qcom,out-strength = <1>;	/* QPNP_PIN_OUT_STRENGTH_LOW */
		qcom,master-en = <0>;
	};
};

&pm8841_mpps {

	mpp@a000 { /* MPP 1 */
		qcom,mode = <6>;
		qcom,master-en = <1>;
	};

	mpp@a100 { /* MPP 2 */
		qcom,mode = <6>;
		qcom,master-en = <1>;
	};

	mpp@a200 { /* MPP 3 */
		qcom,mode = <6>;
		qcom,master-en = <1>;
	};

	mpp@a300 { /* MPP 4 */
		qcom,mode = <6>;
		qcom,master-en = <1>;
	};
};

